# Configuration Firebase
VITE_FIREBASE_API_KEY=votre_cle_api
VITE_FIREBASE_AUTH_DOMAIN=votre_projet.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=votre_projet
VITE_FIREBASE_STORAGE_BUCKET=votre_projet.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=votre_sender_id
VITE_FIREBASE_APP_ID=votre_app_id

# Configuration YouTube Data API
VITE_YOUTUBE_API_KEY=votre_cle_api_youtube
VITE_YOUTUBE_CHANNEL_ID=ID_DE_LA_CHAINE_YOUTUBE

# Configuration Admin
VITE_ADMIN_EMAILS=admin1@example.com,admin2@example.com

# URLs
VITE_API_BASE_URL=https://votre-api.com
VITE_APP_URL=https://votresite.com

# Paramètres de l'application
VITE_APP_NAME="Sarah Jane Iffra"
VITE_APP_DESCRIPTION="Site officiel de Sarah Jane Iffra - Artiste et musicienne"
VITE_APP_THEME_COLOR=#4a148c

# Désactiver les logs en production
VITE_LOG_LEVEL=debug # debug, info, warn, error, silent

# Configuration du cache (en secondes)
VITE_CACHE_TTL=3600 # 1 heure
